<div class="row">
  <div class="col-lg-12">
    <h1 class="page-header">Listing Transactions</h1>
  </div>
</div>

<div class="panel panel-default">
  <div class="panel-body">
    <div class="dataTable_wrapper">
      <table class="table table-striped table-bordered table-hover" id="transactions" style="width: 100%;">
        <thead>
          <tr>
            <th>ID</th>
            <th>Patron ID</th>
            <th>Book ID</th>
            <th>Type</th>
            <th>Date/Time</th>
          </tr>
        </thead>
        <tbody>
          <% @transactions.each do |t| %>
            <tr>  
              <td><%= t.id %></td>
              <td><%= t.patron_id %></td>
              <td><%= t.book_id %></td>
              <td><%= t.transaction_type_id %></td>
              <td><%= t.updated_at %></td>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
